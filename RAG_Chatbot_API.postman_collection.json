{
	"info": {
		"_postman_id": "rag-chatbot-simple-api",
		"name": "RAG Chatbot - Simple Admin API",
		"description": "API đơn giản để xem danh sách tài liệu và quản lý",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Health Check",
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "http://localhost:8000/health",
					"protocol": "http",
					"host": ["localhost"],
					"port": "8000",
					"path": ["health"]
				}
			}
		},
		{
			"name": "List Documents",
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "http://localhost:8000/documents",
					"protocol": "http",
					"host": ["localhost"],
					"port": "8000",
					"path": ["documents"]
				}
			}
		},
		{
			"name": "Delete Document",
			"request": {
				"method": "DELETE",
				"header": [],
				"url": {
					"raw": "http://localhost:8000/documents/{{document_id}}",
					"protocol": "http",
					"host": ["localhost"],
					"port": "8000",
					"path": ["documents", "{{document_id}}"]
				}
			}
		}
	],
	"variable": [
		{
			"key": "document_id",
			"value": "your-document-id-here"
		}
	]
}
